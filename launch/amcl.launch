<launch>
  <arg name="map_file" />

  <node name="map_server"
    pkg="map_server"
    type="map_server"
    args="$(arg map_file)" />

  <node name="amcl"
    pkg="amcl"
    type="amcl"
    clear_params="true">
    <rosparam command="load" file="$(find leo_navigation)/config/amcl.yaml" />
  </node>
</launch>